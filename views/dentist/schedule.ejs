<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>Schedule</title>
  <link rel="stylesheet" href="/css/dentist-dashboard.css"/>
</head>
<body>
  <div class="container">
    <div class="sidebar">
      <!-- Sidebar content (เหมือนเดิม) -->
    </div>

    <div class="main">
      <h1>Your Schedule</h1>
      <table>
        <thead>
          <tr>
            <th>Date</th>
            <th>Time</th>
            <th>Treatment</th>
          </tr>
        </thead>
        <tbody>
          <% scheduleData.forEach(function(schedule) { %>
            <tr>
              <td><%= schedule.date %></td>
              <td><%= schedule.time %></td>
              <td><%= schedule.treatment_name %></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</body>
</html>
